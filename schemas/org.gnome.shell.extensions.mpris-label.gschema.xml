<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.mpris-label" path="/org/gnome/shell/extensions/mpris-label/">

    <key name="left-padding" type="i">
      <default>30</default>
    </key>

    <key name="right-padding" type="i">
      <default>30</default>
    </key>

    <key name="max-string-length" type="i">
      <default>30</default>
    </key>

    <key name="extension-index" type="i">
      <default>3</default>
    </key>

    <key name="extension-place" type="s">
      <default>"right"</default>
    </key>

    <key name="refresh-rate" type="i">
      <default>300</default>
    </key>

    <key name="button-placeholder" type="s">
      <default>"ï¼¿"</default>
    </key>

    <key name="remove-remaster-text" type="b">
      <default>true</default>
    </key>

    <key name="divider-string" type="s">
      <default>" | "</default>
    </key>

    <key name="first-field" type="s">
      <default>"xesam:artist"</default>
    </key>

    <key name="second-field" type="s">
      <default>"xesam:album"</default>
    </key>

    <key name="last-field" type="s">
      <default>"xesam:title"</default>
    </key>

    <key name="remove-text-when-paused" type="b">
      <default>true</default>
    </key>

    <key name="remove-text-paused-delay" type="i">
      <default>3</default>
    </key>

    <key name="auto-switch-to-most-recent" type="b">
      <default>false</default>
    </key>

    <key name="show-icon" type="s">
      <default>"left"</default>
    </key>

    <key name="reposition-delay" type="i">
      <default>2</default>
    </key>

    <key name="reposition-on-button-press" type="b">
      <default>false</default>
    </key>

    <key name="mpris-sources-blacklist" type="s">
      <default>""</default>
    </key>

    <key name="mpris-sources-whitelist" type="s">
      <default>""</default>
    </key>

    <key name="use-whitelisted-sources-only" type="b">
      <default>false</default>
    </key>

    <key name="left-click-action" type="s">
      <default>"open-menu"</default>
    </key>

    <key name="middle-click-action" type="s">
      <default>"play-pause"</default>
    </key>

    <key name="right-click-action" type="s">
      <default>"activate-player"</default>
    </key>

    <key name="volume-control-scheme" type="s">
      <default>"application"</default>
    </key>

    <key name="scroll-up-action" type="s">
      <default>"volume-up"</default>
    </key>

    <key name="scroll-down-action" type="s">
      <default>"volume-down"</default>
    </key>

  </schema>
</schemalist>
